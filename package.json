{
  "name": "muithew-monorepos",
  "private": true,
  "scripts": {
    "build": "turbo build --no-cache",
    "dev": "turbo dev --no-cache  --continue --concurrency 11",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "next": "^14.0.4",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.4"
  },
  "devDependencies": {
    "@configs/eslint": "*",
    "@configs/typescript": "*",
    "prettier": "^3.1.1",
    "turbo": "latest",
    "tsup": "^8.0.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.21",
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/configs/*",
    "packages/core/*",
    "packages/styles/*",
    "packages/utils/*"
  ]
}
