{
  "name": "muithew-monorepos",
  "private": true,
  "description": "muithew-monorepos",
  "author": "annminn100",
  "keywords": [
    "react",
    "nextjs",
    "remix",
    "astro",
    "gatsby",
    "turbo",
    "docker",
    "mui",
    "material",
    "monorepos"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --parallel --concurrency 15",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,js}\"",
    "storybook": "turbo run storybook --parallel",
    "storybook:build": "turbo run storybook:build --parallel",
    "storybook:move": "turbo run storybook:move --filter=docs && turbo run storybook:move --filter=!docs",
    "clean:storybook": "rm -rf storybook-static",
    "clean:build": "turbo clean:build",
    "start": "turbo run start --parallel",
    "prepare": "husky install && bun run --cwd packages build"
  },
  "packageManager": "yarn@1.1.22",
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mdx-js/react": "^3.0.1",
    "@mui/material": "^5.15.11",
    "@mui/material-nextjs": "^5.15.11",
    "@turbo/gen": "^1.11.3",
    "eslint": "^8.55.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "next": "^14.0.4",
    "prettier": "^3.1.1",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.2",
    "sharp": "^0.33.3",
    "storybook": "^7.6.9",
    "tsup": "^8.0.1",
    "turbo": "^1.12.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@configs/eslint": "*",
    "@configs/typescript": "*",
    "@hookform/resolvers": "^3.3.4",
    "@next/eslint-plugin-next": "^14.0.4",
    "@storybook/addon-essentials": "^7.6.8",
    "@storybook/addon-interactions": "^7.6.8",
    "@storybook/addon-links": "^7.6.8",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/addons": "^7.6.8",
    "@storybook/blocks": "^7.6.9",
    "@storybook/nextjs": "^7.6.9",
    "@storybook/react": "^7.6.8",
    "@storybook/react-vite": "^7.6.8",
    "@storybook/test": "^7.6.9",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vercel/style-guide": "^5.1.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^1.11.3",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "framer-motion": "^11.0.15",
    "lint-staged": "^15.2.2",
    "next-intl": "^3.9.1",
    "nodemailer": "^6.9.12",
    "react-hook-form": "^7.51.0",
    "storybook-addon-react-router-v6": "^2.0.10",
    "swiper": "^11.0.7",
    "usehooks-ts": "^3.0.1",
    "vite-tsconfig-paths": "^4.2.1",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/configs/*",
    "packages/core/*",
    "packages/styles/*",
    "packages/utils/*"
  ],
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown",
      "git add ."
    ]
  }
}
